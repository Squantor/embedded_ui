{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build embedded ui mini",
            "type": "shell",
            "command": "make -j18 PROJECT=embedded_ui_mini",
            "group": {
                "kind": "build"
            },
            "problemMatcher": "$gcc"
        },
        {
            "label": "build embedded_ui",
            "type": "shell",
            "command": "make -j18 PROJECT=embedded_ui",
            "group": {
                "kind": "build"
            },
            "problemMatcher": "$gcc"
        },
        {
            "label": "clean",
            "type": "shell",
            "command": "make  PROJECT=embedded_ui clean",
            "problemMatcher": [
                "$gcc"
            ]
        }
    ]
}